{
  "name": "@integralrsg/icubicle",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist node_modules/.cache/tsbuildinfo/tsconfig.tsbuild.info",
    "dev": "vite --config vite.config.ts --mode development",
    "build": "npm run clean && vite build --mode production && npm run sync",
    "typecheck": "tsc -b && tsc -p tsconfig.node.json --noEmit",
    "sync": "echo $ICUBICLE_STATIC_DIR && rsync -avz --delete --mkpath dist \"${ICUBICLE_STATIC_DIR:-/home/projects/www/ibest-root/iCubicle/static/vite_build/}\""
  },
  "dependencies": {
    "@integralrsg/iuicomponents": "0.1.0",
    "@react-three/drei": "^10.7.6",
    "@react-three/fiber": "^9.4.0",
    "chart.js": "^4.5.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "react": "^19.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.0",
    "three": "^0.181.1"
  },
  "devDependencies": {
    "@eslint/css": "^0.14.1",
    "@eslint/js": "^9.39.1",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/three": "^0.181.0",
    "@vitejs/plugin-react": "^5.1.0",
    "check-unused-css": "^0.2.3",
    "eslint": "^9.39.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.3",
    "vite": "^7.2.2"
  }
}
