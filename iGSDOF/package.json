{
  "name": "@integralrsg/igsdof",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist node_modules/.cache/tsbuildinfo/tsconfig.tsbuild.info",
    "dev": "vite --config vite.config.ts --mode development",
    "build": "npm run clean && vite build --mode production && npm run sync",
    "typecheck": "tsc -b && tsc -p tsconfig.node.json --noEmit",
    "sync": "echo $IGSDOF_STATIC_DIR && rsync -avz --delete --mkpath dist \"${IGSDOF_STATIC_DIR:-/home/projects/www/ibest-root/iGSDOF/static/vite_build/}\""
  },
  "dependencies": {
    "@integralrsg/imath": "0.1.0",
    "@integralrsg/iuicomponents": "0.1.0",
    "chart.js": "^4.4.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "webgl-plot": "^0.7.2"
  },
  "devDependencies": {
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.2"
  }
}
