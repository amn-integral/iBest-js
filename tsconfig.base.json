{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@integralrsg/imath": ["iMath/src"],
      "@integralrsg/iuicomponents": ["iUIComponents/src"],
      "@integralrsg/icalcpad": ["iCalcPad/src"],
    }
  }
}
